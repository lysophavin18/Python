def alcohol(age, money):
              if(age >= 21) and (money >= 5):
                 return "We're getting a Drink!"
              elif(age >= 21 ) and (money < 5):
                 return "Come back with more Money."
              elif(age < 21) and (money >= 5):
                  return "Nice,try kid!" 
              else:
                  return "you're too young and too poor."

print("-----------\tCondition can Drink alcohol\t--------------")                 
print("====================================================")
print("if you greater than 25 and have money 5$")  
print(alcohol(21,5))
print("====================================================")
print("if you greater than 25 and you have money 4$")
print(alcohol(21,4))
print("====================================================")
print("if you smalless than 25 and you have money 5$")
print(alcohol(20,5))
print("====================================================")
print("if you 20 years olds and you have money 4$")
print(alcohol(20,4))
