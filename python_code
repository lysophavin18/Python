def Drink(money):
      if money >= 2: 
            return "you got yourself a Drink!"
      else: 
            return "NO Drink for You bro!"
            
            
print("if you have 3$")
print(Drink(3))
print("=============================")
print("if you have 1$")
print(Drink(1))   
